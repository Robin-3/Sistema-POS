{
  "singleQuote": false,
  "jsxSingleQuote": false,
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": true,
  "printWidth": 80,
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "singleQuote": false
      }
    },
    {
      "files": "*.mjs",
      "options": {
        "singleQuote": false
      }
    }
  ]
}
